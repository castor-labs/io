{
    "name": "castor/io",
    "description": "Input/Output interfaces for PHP inspired in Golang",
    "minimum-stability": "stable",
    "license": "BSD-3-Clause",
    "keywords": ["php", "io", "golang", "php7"],
    "authors": [
        {
            "name": "Matias Navarro Carter",
            "email": "mnavarrocarter@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4"
    },
    "autoload": {
        "psr-4": {
            "Castor\\Io\\": "src"
        },
        "files": [
          "functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Castor\\Io\\": "tests"
        }
    },
    "config": {
        "platform": {
            "php": "7.4"
        }
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpunit/phpunit": "^9.5",
        "vimeo/psalm": "^4.6"
    }
}
